$("#minecraft_btn").click(function(){
		// displays an alert
		var txt;
		var r = confirm("You are about to leave the current website. Continue?");
		if (r == true) {
			var url= "https://minecraft.net/en-us/"; 
			window.location = url;
		} else {
			var url= "https://s3-us-west-2.amazonaws.com/cis3110-a4/acePhase4CS_HTML.html"; 
			window.location = url;
		}
    });
