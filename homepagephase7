for(var i=0; i < images.length; i++)
 {
 //<!--Appends image to Div-->
  url = "https://s3-us-west-2.amazonaws.com/cis3110-a4/" + images[i];
  var boxDiv = boxDivs[i];
  var box = document.createElement("div");
  box.setAttribute("class", boxDiv);
  var imageID = imageIDs[i];
  var imageURL = imageURLs[i];
  var anchor = document.createElement("a");
  anchor.setAttribute("id", imageID);
  anchor.setAttribute("href", imageURL);
  var img = document.createElement("img");
  img.setAttribute("class","pctImg");
  img.setAttribute("title", name);
  img.setAttribute("src", url);
  anchor.appendChild(img);

 //!--Appends the title(h3) and short description-->
  var name = names[i];
  var divInBox = document.createElement("div");
  divInBox.setAttribute("class", name);
  var h3 = document.createElement("h3");
  var h3Value = document.createTextNode(name);
  h3.appendChild(h3Value);
  divInBox.appendChild(h3);
  var pInDiv = document.createElement("p");
  pInDiv.setAttribute("class", name);
  var pValue = pValues[i];
  var pInDivValue = document.createTextNode(pValue);
  pInDiv.appendChild(pInDivValue);
  divInBox.appendChild(pInDiv);
  var br = document.createElement("br");
  divInBox.appendChild(br);
  
 //<!--Appends buttons (fav and unfav)-->
  var firstBtnNum = firstBtnNums[i];
  var favBtn = document.createElement("button");
  favBtn.setAttribute("type","button");
  favBtn.setAttribute("class","btn btn-secondary");
	favBtn.setAttribute("id", firstBtnNum);
	favBtn.setAttribute("data-toggle","tooltip");
	favBtn.setAttribute("data-placement","top");
	favBtn.setAttribute("title","Hi");
  var favBtnValue = document.createTextNode("Make favorite");
  favBtn.appendChild(favBtnValue);
  var secondBtnNum = secondBtnNums[i];
  var unfavBtn = document.createElement("button");
  unfavBtn.setAttribute("type","button");
  unfavBtn.setAttribute("class","btn btn-secondary");
	unfavBtn.setAttribute("id", secondBtnNum);
	unfavBtn.setAttribute("data-toggle","popover");
	unfavBtn.setAttribute("data-container","body");
	unfavBtn.setAttribute("data-placement","top");
	unfavBtn.setAttribute("title","Hello");
  var unfavBtnValue = document.createTextNode("Unfavorite");
  unfavBtn.appendChild(unfavBtnValue);
  divInBox.appendChild(favBtn);
  divInBox.appendChild(unfavBtn);
  
 //<!--Image and textbox alternating-->
  if (i==0 || i==2 || i==4) {
	box.appendChild(anchor);
	box.appendChild(divInBox);
  } else if (i==1 || i==3 || i==5) {
	box.appendChild(divInBox);
	box.appendChild(anchor);
  } else {
	box.appendChild(anchor);
	box.appendChild(anchor);
  }
  containMe.appendChild(box);
 
	 
 <!--Jumbotron starts-->
