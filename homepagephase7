var cURL = getCookie();
var heart_icon = document.createElement("img");
var heart_icon_URL = "https://s3-us-west-2.amazonaws.com/cis3110-a4/heart_icon.png";
heart_icon.setAttribute("class", "favPng");
heart_icon.setAttribute("src", heart_icon_URL);

function setCookie(cname,cvalue,exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    var expires = "expires=" + d.toGMTString();
    document.cookie = cname+"= "+cvalue+";  "+expires;
}

function getCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1);
        if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
        }
    }
    return "";
}

function checkCookie() {
  var btn;
  var detail;
  var fav = getCookie("URL");
  // if cookie exist, check the following via switch() 
  if (fav != "") {
	  switch(fav)
	  {
	    // if cookie URL is as such... do the following
		case "https://s3-us-west-2.amazonaws.com/cis3110-a4/minecraft_mod.png":
		// pass btn1 to btn via document.getElementById()
		btn = document.getElementById("btn1");
		// JQuery to hide btn1 and replace it with the heart_icon like it should have been before user left website, essentially a redo
		$("#btn1").hide();
		btn1.parentNode.appendChild(heart_icon);
		// we will also pass the href of a1 (detail page of image 1) via document.getElementById() and append"&fav=1" to var detail
		// this will be used later by detailPage of bucket2 to set the heart_icon on the detail page
		detail = document.getElementById("a1").href + "&fav=1";
		console.log("Page loaded favorite:" + detail);
		// passes "detail" as href to a1 or default1
		// so this line does the passing of web address to the detailPage
		$("#a1").attr("href", detail);
		break;
		case "https://s3-us-west-2.amazonaws.com/cis3110-a4/fallout4_mod.png":
		btn = document.getElementById("btn3");
		$("#btn3").hide();
		btn3.parentNode.appendChild(heart_icon);
		detail = document.getElementById("a2").href + "&fav=1";
		console.log("Page loaded favorite:" + detail);
		$("#a2").attr("href", detail);
		break;
		case "https://s3-us-west-2.amazonaws.com/cis3110-a4/destiny2_mod.png":
		btn = document.getElementById("btn5");
		$("#btn5").hide();
		btn5.parentNode.appendChild(heart_icon);
		detail = document.getElementById("a3").href + "&fav=1";
		console.log("Page loaded favorite:" + detail);
		$("#a3").attr("href", detail);
		break;
		case "https://s3-us-west-2.amazonaws.com/cis3110-a4/dontstarve_mod.png":
		btn = document.getElementById("btn7");
		$("#btn7").hide();
		btn7.parentNode.appendChild(heart_icon);
		detail = document.getElementById("a4").href + "&fav=1";
		console.log("Page loaded favorite:" + detail);
		$("#a4").attr("href", detail);
		break;
		case "https://s3-us-west-2.amazonaws.com/cis3110-a4/leagueoflegends_mod.png":
		btn = document.getElementById("btn9");
		$("#btn9").hide();
		btn9.parentNode.appendChild(heart_icon);
		detail = document.getElementById("a5").href + "&fav=1";
		console.log("Page loaded favorite:" + detail);
		$("#a5").attr("href", detail);
		break;
		case "https://s3-us-west-2.amazonaws.com/cis3110-a4/fortnite_mod.png":
		btn = document.getElementById("btn11");
		$("#btn11").hide();
		btn11.parentNode.appendChild(heart_icon);
		detail = document.getElementById("a6").href + "&fav=1";
		console.log("Page loaded favorite:" + detail);
		$("#a6").attr("href", detail);
		break;
		default:
	  }
  } else {
	// on the other hand, if fav is NOT empty, but the value is null ... setCookie()
	 if (fav != "" && fav != null) {
		 setCookie("URL", fav, 30,checkCookie);
	 }
  }
}

// this section is responsible for excecuting procedures when a btn is clicked
$(document).ready(function(){
	// set all box image boolean state to false	so that we can control the "Unfavorite" function of each box individually
	var box1Image = false;
	var box2Image = false;
	var box3Image = false;
	var box4Image = false;
	var box5Image = false;
	var box6Image = false;
    // when btn1(favorite btn) is clicked
	$("#btn1").click(function(){
		// displays an alert
		alert("Minecraft has been marked as the new favorite.");
		// hide btn1 and show other "favorite" btns just in case any were previously clicked
		$("#btn1").hide();
		$("#btn3").show();
		$("#btn5").show();
		$("#btn7").show();
		$("#btn9").show();
		$("#btn11").show();
		// the parentNide where btn1 was ... now append the heart_icon as child via appendChild
		btn1.parentNode.appendChild(heart_icon);
		// when btn1 is clicked, set box1Image state to true, so that it is responsive to btn2 ("Unfavorite") and only btn2
		// set all other box images to false in case any other box "favorite" btn was clicked before (which would cause it's box image state to be true: and now its false)
		// the purpose is so that each "Unfavorite" btn is only responsive to its "Favorite" btn
		box1Image = true;
		box2Image = false;
		box3Image = false;
		box4Image = false;
		box5Image = false;
		box6Image = false;
		// if btn1 is clicked, pass the respective image address to cURL to be used to set cookie
		cURL = "https://s3-us-west-2.amazonaws.com/cis3110-a4/minecraft_mod.png";
		setCookie("URL",cURL, 30);
		console.log("cURL =" + cURL);
		// represent the state of the cookie
		cookieChecker = 1;
		// set detail address to default1 plus "&fav=1" and passes it to a1 as href
		detail = default1 + "&fav=1";
		$("#a1").attr("href", detail);
    });
     // when btn2 (unfavorite) is clicked 
    $("#btn2").click(function(){
		// displays an alert
		alert("Minecraft has been unmarked from favorite.");
		// shows btn1 again once btn2 (unfavorite) is clicked
		$("#btn1").show();
		// remove the heart_icon
		if (box1Image == true)
		{
			heart_icon.remove(heart_icon);
			box1Image = false;
		}
		// set cURL to empty
		cURL = "";
		setCookie("URL",cURL, 30);
		// change cookieState to 0
		cookiechecker = 0;
		// update a1 to default1 
		$("#a1").attr("href", default1);
    });
	$("#btn3").click(function(){
		alert("Fallout 4 has been marked as the new favorite.");
		$("#btn3").hide();
		$("#btn1").show();
		$("#btn5").show();
		$("#btn7").show();
		$("#btn9").show();
		$("#btn11").show();
		btn3.parentNode.appendChild(heart_icon);
		box2Image = true;
		box1Image = false;
		box3Image = false;
		box4Image = false;
		box5Image = false;
		box6Image = false;
		cURL = "https://s3-us-west-2.amazonaws.com/cis3110-a4/fallout4_mod.png";
		setCookie("URL",cURL, 30);
		console.log("cURL =" + cURL);
		cookieChecker = 1;
		detail = default2 + "&fav=1";
		$("#a2").attr("href", detail);		
    });
    $("#btn4").click(function(){
		alert("Fallout 4 has been unmarked from favorite.");
		$("#btn3").show();
		if (box2Image == true)
		{
			heart_icon.remove(heart_icon);
			box2Image = false;
		}
		cURL = "";
		setCookie("URL",cURL, 30);
		cookiechecker = 0;
		$("#a2").attr("href", default2);
		
