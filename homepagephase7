  var h1 = document.createElement("h1");
  h1.setAttribute("class","display-4");
  // jHeader will return the value of each h1 element from the jHeaders array
  // we will append the h1Value to h1, and append h1 to jDivContainer
  var jHeader = jHeaders[i];
  var h1Value = document.createTextNode(jHeader);
  h1.appendChild(h1Value);
  jDivContainer.appendChild(h1);
  jDiv.appendChild(jDivContainer);
 <!--Jumbotron Header ends-->
 
	 
  <!--Jumbotron Body starts-->
  // jP represents jumbotron Paragraph. This section will be the description of each game
