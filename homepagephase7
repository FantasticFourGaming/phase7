		$("#btn3").show();
		$("#btn5").show();
		$("#btn7").show();
		$("#btn11").show();
		btn9.parentNode.appendChild(heart_icon);
		box5Image = true;
		box1Image = false;
		box2Image = false;
		box3Image = false;
		box4Image = false;
		box6Image = false;
		cURL = "https://s3-us-west-2.amazonaws.com/cis3110-a4/leagueoflegends_mod.png";
		setCookie("URL",cURL, 30);
		console.log("cURL =" + cURL);
		cookieChecker = 1;
		detail = default5 + "&fav=1";
		$("#a5").attr("href", detail);		
    });
    $("#btn10").click(function(){
		alert("League of Legends has been unmarked from favorite.");
		$("#btn9").show();
		if (box5Image == true)
		{
			heart_icon.remove(heart_icon);
			box5Image = false;
		}
		cURL = "";
		setCookie("URL",cURL, 30);
		cookiechecker = 0;
		$("#a5").attr("href", default5);
    });
	$("#btn11").click(function(){
		alert("Fortnite has been marked as the new favorite.");
		$("#btn11").hide();
		$("#btn1").show();
		$("#btn3").show();
		$("#btn5").show();
		$("#btn7").show();
		$("#btn9").show();
		btn11.parentNode.appendChild(heart_icon);
		box6Image = true;
		box1Image = false;
		box2Image = false;
		box3Image = false;
		box4Image = false;
		box5Image = false;
		cURL = "https://s3-us-west-2.amazonaws.com/cis3110-a4/fortnite_mod.png";
		setCookie("URL",cURL, 30); 
		console.log("cURL =" + cURL);
		cookieChecker = 1;
		detail = default6 + "&fav=1";
		$("#a6").attr("href", detail);
    });
    $("#btn12").click(function(){
		alert("Fortnite has been unmarked from favorite.");
		$("#btn11").show();
		if (box6Image == true)
		{
			heart_icon.remove(heart_icon);
			box6Image = false;
		}
		cURL = "";
		setCookie("URL",cURL, 30);
		cookiechecker = 0;
		$("#a6").attr("href", default6);
    });
})
</script>
